{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.ignorePaths": [
    "package-lock.json",
    "yarn.lock",
    "go.sum",
    "firebase.js",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders"
  ],
  "cSpell.words": [
    "actionlogs",
    "antd",
    "aserto",
    "bradlc",
    "bson",
    "createdb",
    "cssinjs",
    "dbaeumer",
    "dbname",
    "dropdb",
    "Equalf",
    "fluentui",
    "frontends",
    "fullscreen",
    "gluestack",
    "gofiber",
    "gorm",
    "icns",
    "joho",
    "Kolkata",
    "netinfo",
    "Pressable",
    "sslmode",
    "stretchr",
    "swaggo",
    "tanstack",
    "thoas",
    "Uppy",
    "usergroups"
  ]
}
